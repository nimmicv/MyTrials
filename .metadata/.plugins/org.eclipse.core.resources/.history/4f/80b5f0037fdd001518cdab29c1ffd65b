package com.technical.generic;

public class BinarySearch {
	public static void main(String[] args)
	{
		int[] array = {3,6,8,10,34,34,34,34,50,100,80};
		binary_search_repeated(array,34);
	}
	public static int binary_search_repeated(int[] array,int item)
	{
		int startindex=-1;
		int endindex =-1;
		int lb = 0;
		int ub = array.length - 1;
		while(lb<=ub)
		{
			int mid = lb +(ub-lb)/2;
			if(item>array[mid])
			{
				lb=mid+1;
			}
			else
			if(array[mid]<=item)
			{
				if(array[mid]==item)
					startindex = mid;
				ub = mid-1;
			}

			
		}
		System.out.println("Start index = "+startindex);
		return startindex;
	}
	
	

}
