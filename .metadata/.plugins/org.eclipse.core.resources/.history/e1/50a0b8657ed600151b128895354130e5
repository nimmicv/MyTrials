package com.technical.generic;

import java.util.Stack;

public class MinimumValDiff {
	int val1 = -1;
	int val2 = -1;
	int diff = Integer.MAX_VALUE;
	
	public void minimumDiff(Tree node)
	{
		Stack<Tree> stack = new Stack<Tree>();
		while(node!=null)
		{
			stack.add(node);
			node = node.left;
		}
		while(!stack.isEmpty())
		{
			Tree tree_node = stack.pop();
			
			
		}
		
	}

}
