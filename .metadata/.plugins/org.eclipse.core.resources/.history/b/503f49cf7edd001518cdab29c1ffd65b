package com.technical.generic;

public class BinarySearch {
	
	public int binary_search_repeated(int[] array,int item)
	{
		int startindex=-1;
		int endindex =-1;
		int lb = 0;
		int ub = array.length - 1;
		while(lb<=ub)
		{
			int mid = lb +(ub-lb)/2;
			if(item>array[mid])
			{
				lb=mid+1;
			}
			else
			if(array[mid]<=item)
			{
				if(array[mid]==item)
					startindex = mid;
				ub = mid-1;
			}

			
		}
		System.out.println("Start index = "+array[mid]<=item));
	}
	
	

}
